<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashScreenFirst">

    <ImageView
        android:id="@+id/wallet_8_1"
        android:layout_width="256dp"
        android:layout_height="256dp"
        android:background="@drawable/wallet81"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <TextView
        android:id="@+id/damen_walle"
        android:layout_width="198dp"
        android:layout_height="42dp"
        android:layout_marginBottom="200dp"
        android:layout_marginTop="56dp"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="4sp"
        android:text="@string/damen_walle"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="#1C3564"
        android:translationY="-2.25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/wallet_8_1"
        app:layout_constraintStart_toStartOf="@+id/wallet_8_1"
        app:layout_constraintTop_toBottomOf="@+id/wallet_8_1"
        app:layout_constraintVertical_bias="0.152" />

</androidx.constraintlayout.widget.ConstraintLayout>